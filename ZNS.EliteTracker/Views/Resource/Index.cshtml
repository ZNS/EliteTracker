@model ZNS.EliteTracker.Models.Views.ResourceIndexView
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="ResourceIndex">
    <div class="page-header">
        <h1>Resources</h1>
        <a href="@Url.Action("Edit")"><span class="fa fa-plus-square-o"></span></a>
    </div>
    <div class="items">
        @foreach (var resource in Model.Resources)
        {
            @Html.Partial("Partials/_ResourceItem", resource)
        }
    </div>
    <div class="clearfix">
        @Html.Partial("Partials/_Pager", Model.Pager)
    </div>
</div>